---
import Layout from '../layouts/Layout.astro';
import Footer from '../components/Footer.astro';
import ItemPage from '../components/ItemPage.astro';

const PUBLIC_STRIPE_KEY = import.meta.env.PUBLIC_STRIPE_KEY;
---

<Layout title="Variety Gifts Unlimited - Tanner Stickers">

    <ItemPage
      title="Tanner Stickers"
      bodyText="Meet Tanner, our husky mascot for Variety Gifts Unlimited! Let Tanner bring some love into your life with these stickers of her cute face!"
      priceText="$5.99 for 4 stickers with Free Shipping!"
      imageUrl="/tanner/stickers.webp"
      imageAlt="Tanner Sticker"
      stripeKey={PUBLIC_STRIPE_KEY}
      stripePaymentId="price_1PzqLFAp2D4XT14xzkgRqF8T"
      youtubeEmbedId="84YbZWqk3jM"
    />

</Layout>